	$j.fn.activateAutoUpdateIcon = function() {
		if ($j('.cmeUpdateControl').length){
			var myUpdateControl = $j(this).closest('.cmeUpdateControl');
			if (($j('html').hasClass('msie-8')) || $j('html').hasClass('msie-7')){
